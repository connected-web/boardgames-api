{
	"id": "/",
	"type": "object",
	"properties": {
		"byMonth": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"date": {
						"type": "string",
						"pattern": "^\\d{4}(-\\d{2}){2}$"
					},
					"game": {
						"type": "string",
						"pattern": "^.*$"
					},
					"coOp": {
						"type": "string",
						"pattern": "^([Yy]es)|([Nn]o)$"
					},
					"coOpOutcome": {
						"type": "string",
						"pattern": "^([Ww]in)|([Ww]on)|([Ll]os[te])$"
					},
					"note": {
						"type": "string",
						"pattern": "^.*$"
					},
					"winner": {
						"type": "string",
						"pattern": "^.*$"
					}
				},
				"additionalProperties": false
			}
		}
	},
	"byYear": {
		"type": "array",
		"items": {
			"type": "object"
		}
	},
	"earliestDate": {
		"type": "string",
		"pattern": "^\\d{4}-\\d{2}-\\d{2}$"
	},
	"latestDate": {
		"type": "string",
		"pattern": "^\\d{4}-\\d{2}-\\d{2}$"
	},
	"additionalProperties": false
}
